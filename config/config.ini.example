# DO NOT USE NOTEPAD TO EDIT CONFIG FILES!! USE NOTEPAD ++ INSTEAD.
# Authentication settings
#auth-service: ptc         # ptc (default) or google
#username:
#password:

# Database settings
#db-type: mysql        # sqlite (default) or mysql
#db-host: 127.0.0.1              # required for mysql
#db-name:                # required for mysql
#db-user:                # required for mysql
#db-pass:                # required for mysql
#db-port:               # default 3306
#db-max_connections:	# default 5
#db-threads:			# default 1

# Search settings
location: 
speed_limit: 32			# max speed limit in km/hr, varies randomly between 70-100%. Overrides scan-delay
jitter
#no-gyms               # disables gym scanning (default false)
#no-pokemon:            # disables pokemon scanning (default false)
#no-pokestops			# disables pokestop scanning (default false)
ignore-list: []           # list of Pokemon IDs to ignore parsing (default [])
#login-delay: 			# default 6
scan-delay:           # default 10; used for the scan-retry delay
step-limit:            # default 12
#step_distance: 50		# inoperative currently
scan_retries: 2
max-failures: 5
#gym-info:              # enables detailed gym info collection (default false)
#min-seconds-left:      # time that must be left on a spawn before considering it too late and skipping it (default 0)
#status-name:           # enables writing status updates to the database - if you use multiple processes, each needs a unique value
account-rest-interval: 900
beehive
captcha-solving
captcha-key: 

#Pokemon IV 
#encounter
#encounter-delay: 5
#encounter-whitelist: [26,28,38,40,45,51,62,65,68,71,76,82,83,87,89,91,94,101,115,130,132,136,139,141,144,145,146,150,151,3,5,6,9,12,24,30,31,33,34,36,44,53,55,57,59,64,67,73,75,78,80,85,88,99,103,105,106,110,112,113,117,119,121,122,131,134,135,137,142,143,148,149,2,4,8,11,14,15,18,20,22,27,37,39,42,47,49,50,52,54,56,61,63,66,70,72,74,77,79,81,86,90,93,95,97,100,102,104,107,108,109,111,114,116,118,123,124,125,126,128,138,140,147,25,26,129,1,7]
